ge\"\r\n        :length=\"pageLength\"\r\n        rounded=\"circle\"\r\n        @input=\"changePage(page)\"\r\n    ></v-pagination>\r\n  </div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, reactive, ref} from \"vue\";\r\nimport {getUsers, test1} from \"../../../../network/user\";\r\n\r\nexport default {\r\n  name: \"adminManageUsers\",\r\n  setup() {\r\n    let page = ref(1);\r\n    let desserts = reactive([]);\r\n    let pageLength = ref(1);\r\n\r\n    onMounted(()=>{\r\n      init()\r\n    })\r\n\r\n    const init = () => {\r\n      getUsers(page).then(res=>{\r\n        desserts = res.data.data;\r\n        pageLength = res.data.PageLength;\r\n      })\r\n    }\r\n\r\n    const changePage = (page) => {\r\n      getUsers(page).then(res=>{\r\n        desserts = res.data.data;\r\n      })\r\n    }\r\n\r\n    return {\r\n      desserts,\r\n      page,\r\n      changePage,\r\n      pageLength,\r\n      str\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}