{"ast":null,"code":"import { onMounted, ref, watch } from \"vue\";\nimport { getSalers } from \"../../../../network/user\";\nexport default {\n  name: \"adminManageUsers\",\n\n  setup() {\n    let page = ref(1);\n    let desserts = ref([]);\n    let pageLength = ref(1);\n    let selected = ref([]);\n    onMounted(() => {\n      init();\n    });\n\n    const init = () => {\n      getSalers(page.value).then(res => {\n        desserts.value = res.data.data;\n        pageLength.value = res.data.PageLength;\n      });\n    };\n\n    watch(page, async (newPage, oldPage) => {\n      init();\n    });\n    return {\n      desserts,\n      page,\n      pageLength\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6EA,SAAQA,SAAR,EAAmBC,GAAnB,EAAwBC,KAAxB,QAAoC,KAApC;AACA,SAAQC,SAAR,QAAwB,0BAAxB;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;;AAEbC,OAAK,GAAG;AACN,QAAIC,IAAG,GAAIL,GAAG,CAAC,CAAD,CAAd;AACA,QAAIM,QAAO,GAAIN,GAAG,CAAC,EAAD,CAAlB;AACA,QAAIO,UAAS,GAAIP,GAAG,CAAC,CAAD,CAApB;AACA,QAAIQ,QAAO,GAAIR,GAAG,CAAC,EAAD,CAAlB;AAEAD,aAAS,CAAC,MAAI;AACZU,UAAI;AACL,KAFQ,CAAT;;AAIA,UAAMA,IAAG,GAAI,MAAM;AACjBP,eAAS,CAACG,IAAI,CAACK,KAAN,CAAT,CAAsBC,IAAtB,CAA2BC,GAAG,IAAE;AAC9BN,gBAAQ,CAACI,KAAT,GAAiBE,GAAG,CAACC,IAAJ,CAASA,IAA1B;AACAN,kBAAU,CAACG,KAAX,GAAmBE,GAAG,CAACC,IAAJ,CAASC,UAA5B;AACD,OAHD;AAIF,KALA;;AAOAb,SAAK,CAACI,IAAD,EAAO,OAAMU,OAAN,EAAeC,OAAf,KAA2B;AACrCP,UAAI;AACL,KAFI,CAAL;AAIA,WAAO;AACLH,cADK;AAELD,UAFK;AAGLE;AAHK,KAAP;AAKF;;AA5Ba,CAAf","names":["onMounted","ref","watch","getSalers","name","setup","page","desserts","pageLength","selected","init","value","then","res","data","PageLength","newPage","oldPage"],"sourceRoot":"","sources":["C:\\Users\\tpmp1\\Desktop\\vue-project\\minishop\\src\\views\\back\\admin\\adminManageUsers.vue"],"sourcesContent":["<template>\r\n  <v-card style=\"height: 200px;\" color=\"#1F8A70\">\r\n    <v-card-title class=\"text-h4 font-weight-black text-white mt-6 ml-4\">销售人员查询</v-card-title>\r\n    <v-card-subtitle class=\"text-h6 font-weight-black text-white mt-4 ml-4\">\r\n      在此可以查询销售人员的ID、姓名、电子邮箱、手机号码等信息，同时管理员也可以对销售人员的信息进行编辑和删除操作\r\n    </v-card-subtitle>\r\n  </v-card>\r\n  <v-card class=\"mt-6\">\r\n  <v-table>\r\n    <template v-slot:default>\r\n      <thead style=\"background-color: #1F8A70\">\r\n      <tr>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          ID\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          姓名\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          电子邮箱\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          手机号码\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          操作\r\n        </th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr\r\n          v-for=\"item in desserts\"\r\n          :key=\"item.id\"\r\n      >\r\n        <td class=\"font-weight-medium\">{{ item.id }}</td>\r\n        <td class=\"font-weight-medium\">{{ item.name }}</td>\r\n        <td class=\"font-weight-medium\">{{item.email}}</td>\r\n        <td class=\"font-weight-medium\">{{item.phone}}</td>\r\n        <td>\r\n          <v-btn\r\n              color=\"#FD7400\"\r\n              class=\"font-weight-black text-white\"\r\n              plain\r\n          >\r\n            <v-icon>mdi-file-document-edit</v-icon>\r\n            编辑\r\n          </v-btn>\r\n          &nbsp\r\n          <v-btn\r\n              color=\"#FD7400\"\r\n              plain\r\n              class=\"font-weight-black text-white\"\r\n          >\r\n            <v-icon>mdi-file-remove</v-icon>\r\n            删除\r\n          </v-btn>\r\n        </td>\r\n      </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-table>\r\n  </v-card>\r\n  <div class=\"float-right mt-2\">\r\n    <v-pagination\r\n        v-model=\"page\"\r\n        :length=\"pageLength\"\r\n        rounded=\"circle\"\r\n        :total-visible=\"10\"\r\n    ></v-pagination>\r\n  </div>\r\n\r\n  \r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, ref, watch} from \"vue\";\r\nimport {getSalers} from \"../../../../network/user\";\r\n\r\nexport default {\r\n  name: \"adminManageUsers\",\r\n  setup() {\r\n    let page = ref(1);\r\n    let desserts = ref([]);\r\n    let pageLength = ref(1);\r\n    let selected = ref([]);\r\n\r\n    onMounted(()=>{\r\n      init()\r\n    })\r\n\r\n    const init = () => {\r\n      getSalers(page.value).then(res=>{\r\n        desserts.value = res.data.data;\r\n        pageLength.value = res.data.PageLength;\r\n      })\r\n    }\r\n\r\n    watch(page, async(newPage, oldPage) => {\r\n      init()\r\n    })\r\n\r\n    return {\r\n      desserts,\r\n      page,\r\n      pageLength,\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}