{"ast":null,"code":"import { useRouter } from \"vue-router\";\nimport { onMounted, ref, watch } from \"vue\";\nimport { betchDeleteCartById, deleteCartById, getCart } from \"../../../../network/cart\";\nexport default {\n  name: \"salerManageCart\",\n\n  setup() {\n    let router = useRouter();\n    let page = ref(1);\n    let desserts = ref([]);\n    let pageLength = ref(1);\n    let pageSize = ref(1);\n    let pageTotal = ref(1);\n    let selected = ref([]);\n    let fullSelected = ref(false);\n    let deleteSuccess = ref(false);\n    let deleteFailed = ref(false);\n    onMounted(() => {\n      init();\n    });\n\n    const init = () => {\n      getCart(page.value).then(res => {\n        desserts.value = res.data.data;\n        pageLength.value = res.data.PageLength;\n        pageSize.value = res.data.PageSize;\n        pageTotal.value = res.data.PageTotal;\n      });\n    };\n\n    const changeFullSelect = () => {\n      if (fullSelected.value == true) {\n        selected.value = desserts.value.map(item => item.id);\n      } else {\n        selected.value = [];\n      }\n    };\n\n    watch(page, async (newPage, oldPage) => {\n      init();\n    });\n\n    const deleteCart = id => {\n      deleteCartById(id).then(res => {\n        let status = res.data.status;\n\n        if (status > 0) {\n          deleteSuccess.value = true;\n          setTimeout(() => {\n            deleteSuccess.value = false;\n          }, 1000);\n        } else {\n          deleteFailed.value = true;\n          setTimeout(() => {\n            deleteFailed.value = false;\n          }, 1000);\n        }\n\n        init();\n      });\n    };\n\n    const batchDeleteCart = () => {\n      betchDeleteCartById(selected.value).then(res => {\n        let status = res.data.status;\n\n        if (status > 0) {\n          deleteSuccess.value = true;\n          setTimeout(() => {\n            deleteSuccess.value = false;\n          }, 1000);\n        } else {\n          deleteFailed.value = true;\n          setTimeout(() => {\n            deleteFailed.value = false;\n          }, 1000);\n        }\n\n        init();\n      });\n    };\n\n    return {\n      desserts,\n      page,\n      pageLength,\n      selected,\n      changeFullSelect,\n      fullSelected,\n      pageSize,\n      deleteSuccess,\n      deleteFailed,\n      deleteCart,\n      batchDeleteCart\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgFA,SAAQA,SAAR,QAAwB,YAAxB;AACA,SAAQC,SAAR,EAAmBC,GAAnB,EAAwBC,KAAxB,QAAoC,KAApC;AACA,SAAQC,mBAAR,EAA6BC,cAA7B,EAA6CC,OAA7C,QAA2D,0BAA3D;AAEA,eAAe;AACbC,MAAI,EAAE,iBADO;;AAEbC,OAAK,GAAG;AAEN,QAAIC,MAAK,GAAIT,SAAS,EAAtB;AACA,QAAIU,IAAG,GAAIR,GAAG,CAAC,CAAD,CAAd;AACA,QAAIS,QAAO,GAAIT,GAAG,CAAC,EAAD,CAAlB;AACA,QAAIU,UAAS,GAAIV,GAAG,CAAC,CAAD,CAApB;AACA,QAAIW,QAAO,GAAIX,GAAG,CAAC,CAAD,CAAlB;AACA,QAAIY,SAAQ,GAAIZ,GAAG,CAAC,CAAD,CAAnB;AACA,QAAIa,QAAO,GAAIb,GAAG,CAAC,EAAD,CAAlB;AACA,QAAIc,YAAW,GAAId,GAAG,CAAC,KAAD,CAAtB;AACA,QAAIe,aAAY,GAAIf,GAAG,CAAC,KAAD,CAAvB;AACA,QAAIgB,YAAW,GAAIhB,GAAG,CAAC,KAAD,CAAtB;AAEAD,aAAS,CAAC,MAAI;AACZkB,UAAI;AACL,KAFQ,CAAT;;AAIA,UAAMA,IAAG,GAAI,MAAM;AACjBb,aAAO,CAACI,IAAI,CAACU,KAAN,CAAP,CAAoBC,IAApB,CAAyBC,GAAG,IAAE;AAC5BX,gBAAQ,CAACS,KAAT,GAAiBE,GAAG,CAACC,IAAJ,CAASA,IAA1B;AACAX,kBAAU,CAACQ,KAAX,GAAmBE,GAAG,CAACC,IAAJ,CAASC,UAA5B;AACAX,gBAAQ,CAACO,KAAT,GAAiBE,GAAG,CAACC,IAAJ,CAASE,QAA1B;AACAX,iBAAS,CAACM,KAAV,GAAkBE,GAAG,CAACC,IAAJ,CAASG,SAA3B;AACD,OALD;AAMF,KAPA;;AASA,UAAMC,gBAAe,GAAI,MAAM;AAC7B,UAAIX,YAAY,CAACI,KAAb,IAAsB,IAA1B,EAAgC;AAC9BL,gBAAQ,CAACK,KAAT,GAAiBT,QAAQ,CAACS,KAAT,CAAeQ,GAAf,CAAmBC,IAAI,IAAEA,IAAI,CAACC,EAA9B,CAAjB;AACF,OAFA,MAEO;AACLf,gBAAQ,CAACK,KAAT,GAAiB,EAAjB;AACF;AACF,KANA;;AAQAjB,SAAK,CAACO,IAAD,EAAO,OAAMqB,OAAN,EAAeC,OAAf,KAA2B;AACrCb,UAAI;AACL,KAFI,CAAL;;AAIA,UAAMc,UAAS,GAAKH,EAAD,IAAQ;AACzBzB,oBAAc,CAACyB,EAAD,CAAd,CAAmBT,IAAnB,CAAwBC,GAAG,IAAE;AAC3B,YAAIY,MAAK,GAAIZ,GAAG,CAACC,IAAJ,CAASW,MAAtB;;AACA,YAAIA,MAAK,GAAI,CAAb,EAAgB;AACdjB,uBAAa,CAACG,KAAd,GAAsB,IAAtB;AACAe,oBAAU,CAAC,MAAI;AACblB,yBAAa,CAACG,