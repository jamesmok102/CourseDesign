{"ast":null,"code":"import { onMounted, ref, watch } from \"vue\";\nimport { deleteSalersById, getSalers } from \"../../../../network/saler\";\nexport default {\n  name: \"adminManageUsers\",\n\n  setup() {\n    let page = ref(1);\n    let desserts = ref([]);\n    let pageLength = ref(1);\n    let pageSize = ref(1);\n    let pageTotal = ref(1);\n    let selected = ref([]);\n    let fullSelected = ref(false);\n    onMounted(() => {\n      init();\n    });\n\n    const init = () => {\n      getSalers(page.value).then(res => {\n        desserts.value = res.data.data;\n        pageLength.value = res.data.PageLength;\n        pageSize.value = res.data.PageSize;\n        pageTotal.value = res.data.PageTotal;\n      });\n    };\n\n    const changeFullSelect = () => {\n      if (fullSelected.value == true) {\n        selected.value = desserts.value.map(item => item.id);\n      } else {\n        selected.value = [];\n      }\n    };\n\n    watch(page, async (newPage, oldPage) => {\n      init();\n    });\n\n    const deleteSaler = id => {\n      deleteSalersById(id).then(res => {\n        let status = res.data.status;\n      });\n    };\n\n    return {\n      desserts,\n      page,\n      pageLength,\n      selected,\n      changeFullSelect,\n      fullSelected,\n      pageSize\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgFA,SAAQA,SAAR,EAAmBC,GAAnB,EAAwBC,KAAxB,QAAoC,KAApC;AACA,SAAQC,gBAAR,EAA0BC,SAA1B,QAA0C,2BAA1C;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;;AAEbC,OAAK,GAAG;AACN,QAAIC,IAAG,GAAIN,GAAG,CAAC,CAAD,CAAd;AACA,QAAIO,QAAO,GAAIP,GAAG,CAAC,EAAD,CAAlB;AACA,QAAIQ,UAAS,GAAIR,GAAG,CAAC,CAAD,CAApB;AACA,QAAIS,QAAO,GAAIT,GAAG,CAAC,CAAD,CAAlB;AACA,QAAIU,SAAQ,GAAIV,GAAG,CAAC,CAAD,CAAnB;AACA,QAAIW,QAAO,GAAIX,GAAG,CAAC,EAAD,CAAlB;AACA,QAAIY,YAAW,GAAIZ,GAAG,CAAC,KAAD,CAAtB;AAEAD,aAAS,CAAC,MAAI;AACZc,UAAI;AACL,KAFQ,CAAT;;AAIA,UAAMA,IAAG,GAAI,MAAM;AACjBV,eAAS,CAACG,IAAI,CAACQ,KAAN,CAAT,CAAsBC,IAAtB,CAA2BC,GAAG,IAAE;AAC9BT,gBAAQ,CAACO,KAAT,GAAiBE,GAAG,CAACC,IAAJ,CAASA,IAA1B;AACAT,kBAAU,CAACM,KAAX,GAAmBE,GAAG,CAACC,IAAJ,CAASC,UAA5B;AACAT,gBAAQ,CAACK,KAAT,GAAiBE,GAAG,CAACC,IAAJ,CAASE,QAA1B;AACAT,iBAAS,CAACI,KAAV,GAAkBE,GAAG,CAACC,IAAJ,CAASG,SAA3B;AACD,OALD;AAMF,KAPA;;AASA,UAAMC,gBAAe,GAAI,MAAM;AAC7B,UAAIT,YAAY,CAACE,KAAb,IAAsB,IAA1B,EAAgC;AAC9BH,gBAAQ,CAACG,KAAT,GAAiBP,QAAQ,CAACO,KAAT,CAAeQ,GAAf,CAAmBC,IAAI,IAAEA,IAAI,CAACC,EAA9B,CAAjB;AACF,OAFA,MAEO;AACLb,gBAAQ,CAACG,KAAT,GAAiB,EAAjB;AACF;AACF,KANA;;AAQAb,SAAK,CAACK,IAAD,EAAO,OAAMmB,OAAN,EAAeC,OAAf,KAA2B;AACrCb,UAAI;AACL,KAFI,CAAL;;AAIA,UAAMc,WAAU,GAAKH,EAAD,IAAQ;AAC1BtB,sBAAgB,CAACsB,EAAD,CAAhB,CAAqBT,IAArB,CAA0BC,GAAG,IAAE;AAC7B,YAAIY,MAAK,GAAIZ,GAAG,CAACC,IAAJ,CAASW,MAAtB;AAED,OAHD;AAIF,KALA;;AAOA,WAAO;AACLrB,cADK;AAELD,UAFK;AAGLE,gBAHK;AAILG,cAJK;AAKLU,sBALK;AAMLT,kBANK;AAOLH;AAPK,KAAP;AASF;;AApDa,CAAf","names":["onMounted","ref","watch","deleteSalersById","getSalers","name","setup","page","desserts","pageLength","pageSize","pageTotal","selected","fullSelected","init","value","then","res","data","PageLength","PageSize","PageTotal","changeFullSelect","map","item","id","newPage","oldPage","deleteSaler","status"],"sourceRoot":"","sources":["C:\\Users\\tpmp1\\Desktop\\vue-project\\minishop\\src\\views\\back\\admin\\adminManageUsers.vue"],"sourcesContent":["<template>\r\n  <v-card style=\"height: 200px;\" color=\"#1F8A70\">\r\n    <v-card-title class=\"text-h4 font-weight-black text-white mt-6 ml-4\">销售人员查询</v-card-title>\r\n    <v-card-subtitle class=\"text-h6 font-weight-black text-white mt-4 ml-4\">\r\n      在此可以查询销售人员的ID、姓名、电子邮箱、手机号码等信息，同时管理员也可以对销售人员的信息进行编辑和删除操作\r\n    </v-card-subtitle>\r\n  </v-card>\r\n  <v-card class=\"mt-6\">\r\n  <v-table>\r\n    <template v-slot:default>\r\n      <thead style=\"background-color: #1F8A70\">\r\n      <tr>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          <v-checkbox hide-details @change=\"changeFullSelect\" v-model=\"fullSelected\"></v-checkbox>\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          姓名\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          电子邮箱\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          手机号码\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          操作\r\n        </th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr\r\n          v-for=\"item in desserts\"\r\n          :key=\"item.id\"\r\n      >\r\n        <td class=\"font-weight-medium\"><v-checkbox hide-details :value=\"item.id\" v-model=\"selected\"></v-checkbox></td>\r\n        <td class=\"font-weight-medium\">{{ item.name }}</td>\r\n        <td class=\"font-weight-medium\">{{item.email}}</td>\r\n        <td class=\"font-weight-medium\">{{item.phone}}</td>\r\n        <td>\r\n          <v-btn\r\n              color=\"#FD7400\"\r\n              class=\"font-weight-black text-white\"\r\n              plain\r\n          >\r\n            <v-icon>mdi-file-document-edit</v-icon>\r\n            编辑\r\n          </v-btn>\r\n          &nbsp\r\n          <v-btn\r\n              color=\"#FD7400\"\r\n              plain\r\n              class=\"font-weight-black text-white\"\r\n              @click=\"deleteSaler(item.id)\"\r\n          >\r\n            <v-icon>mdi-file-remove</v-icon>\r\n            删除\r\n          </v-btn>\r\n          \r\n        </td>\r\n      </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-table>\r\n  </v-card>\r\n  <div class=\"float-right mt-2\">\r\n    <v-pagination\r\n        v-model=\"page\"\r\n        :length=\"pageLength\"\r\n        rounded=\"circle\"\r\n        :total-visible=\"10\"\r\n    ></v-pagination>\r\n  </div>\r\n  <v-snackbar v-model=\"successFlag\"></v-snackbar>\r\n\r\n  \r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, ref, watch} from \"vue\";\r\nimport {deleteSalersById, getSalers} from \"../../../../network/saler\";\r\n\r\nexport default {\r\n  name: \"adminManageUsers\",\r\n  setup() {\r\n    let page = ref(1);\r\n    let desserts = ref([]);\r\n    let pageLength = ref(1);\r\n    let pageSize = ref(1);\r\n    let pageTotal = ref(1);\r\n    let selected = ref([]);\r\n    let fullSelected = ref(false);\r\n\r\n    onMounted(()=>{\r\n      init()\r\n    })\r\n\r\n    const init = () => {\r\n      getSalers(page.value).then(res=>{\r\n        desserts.value = res.data.data;\r\n        pageLength.value = res.data.PageLength;\r\n        pageSize.value = res.data.PageSize;\r\n        pageTotal.value = res.data.PageTotal;\r\n      })\r\n    }\r\n\r\n    const changeFullSelect = () => {\r\n      if (fullSelected.value == true) {\r\n        selected.value = desserts.value.map(item=>item.id)\r\n      } else {\r\n        selected.value = [];\r\n      }\r\n    }\r\n\r\n    watch(page, async(newPage, oldPage) => {\r\n      init()\r\n    })\r\n\r\n    const deleteSaler = (id) => {\r\n      deleteSalersById(id).then(res=>{\r\n        let status = res.data.status;\r\n\r\n      })\r\n    }\r\n\r\n    return {\r\n      desserts,\r\n      page,\r\n      pageLength,\r\n      selected,\r\n      changeFullSelect,\r\n      fullSelected,\r\n      pageSize\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}