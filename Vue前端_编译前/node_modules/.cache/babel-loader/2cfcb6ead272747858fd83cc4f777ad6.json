t:default>\r\n      <thead style=\"background-color: #1F8A70\">\r\n      <tr>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          ID\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          姓名\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          电子邮箱\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          手机号码\r\n        </th>\r\n        <th class=\"text-left text-subtitle-1 font-weight-black text-white\">\r\n          操作\r\n        </th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr\r\n          v-for=\"item in desserts\"\r\n          :key=\"item.id\"\r\n      >\r\n        <td class=\"font-weight-medium\">{{ item.id }}</td>\r\n        <td class=\"font-weight-medium\">{{ item.name }}</td>\r\n        <td class=\"font-weight-medium\">{{item.email}}</td>\r\n        <td class=\"font-weight-medium\">{{item.phone}}</td>\r\n        <td>\r\n          <v-btn\r\n              color=\"#FD7400\"\r\n              class=\"font-weight-black text-white\"\r\n              plain\r\n          >\r\n            <v-icon>mdi-file-document-edit</v-icon>\r\n            编辑\r\n          </v-btn>\r\n          &nbsp\r\n          <v-btn\r\n              color=\"#FD7400\"\r\n              plain\r\n              class=\"font-weight-black text-white\"\r\n          >\r\n            <v-icon>mdi-file-remove</v-icon>\r\n            删除\r\n          </v-btn>\r\n        </td>\r\n      </tr>\r\n      </tbody>\r\n    </template>\r\n  </v-table>\r\n  </v-card>\r\n  <div class=\"float-right mt-2\">\r\n    <v-pagination\r\n        v-model=\"page\"\r\n        :length=\"pageLength\"\r\n        rounded=\"circle\"\r\n        :total-visible=\"10\"\r\n    ></v-pagination>\r\n  </div>\r\n\r\n  {{page}}\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport {nextTick, onMounted, reactive, ref} from \"vue\";\r\nimport {getUsers} from \"../../../../network/user\";\r\n\r\nexport default {\r\n  name: \"adminManageUsers\",\r\n  setup() {\r\n    let page = ref(1);\r\n    let desserts = ref([]);\r\n    let pageLength = ref(1);\r\n\r\n    onMounted(()=>{\r\n      init()\r\n    })\r\n\r\n    const init = () => {\r\n      getUsers(page.value).then(res=>{\r\n        desserts.value = res.data.data;\r\n        pageLength.value = res.data.PageLength;\r\n      })\r\n    }\r\n\r\n\r\n\r\n    return {\r\n      desserts,\r\n      page,\r\n      pageLength,\r\n      changePage\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}